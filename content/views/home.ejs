<!DOCTYPE html>
<html lang="en">
	<!-- header -->
	<%- include('./partials/head') %> 
<body>
	<!-- navbar -->
	<%- include('./partials/nav') %> 

	<div class="body">
		<% Object.values(tours).forEach(function(item){ %>
		<div class="tours">
			<div class="places">
				<h2><%= item.name %></h2>
				<p><%= item.description %></p>
				<br>
				<a href="/tour/<%= item._id %>">Learn more</a>
			</div>
		</div>
		<% }) %>
	</div>
	<!-- footer -->
	<%- include('./partials/footer') %> 

</body>
</html>